#include <stdio.h>


int sumEvenNumbers();

int main() {
    int sum;

   
    sum = sumEvenNumbers();

  
    printf("The sum of the first 10 even natural numbers is: %d\n", sum);

    return 0;
}


int sumEvenNumbers() {
    int i, sum = 0;

   
    for (i = 1; i <= 10; i++) {
        sum += 2 * i;  // even numbers are 2, 4, 6, ..., 20
    }

    return sum;
}
